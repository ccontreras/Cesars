<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="cesars.becrox.com.cesars">

  <application
      android:allowBackup="true"
      android:icon="@mipmap/ic_launcher"
      android:label="@string/app_name"
      android:roundIcon="@mipmap/ic_launcher_round"
      android:supportsRtl="true"
      android:theme="@style/AppTheme.NoActionBar">
    <activity android:name=".ui.bio.BioActivity">
      <intent-filter>
        <action android:name="android.intent.action.MAIN"/>

        <category android:name="android.intent.category.LAUNCHER"/>
      </intent-filter>
    </activity>
    <activity
        android:name=".ui.contact.ContactActivity"
        android:label="@string/title_contact_me_activity"
        android:parentActivityName=".ui.bio.BioActivity">
      <meta-data
          android:name="android.support.PARENT_ACTIVITY"
          android:value=".ui.bio.BioActivity"/>
    </activity>
  </application>

</manifest>